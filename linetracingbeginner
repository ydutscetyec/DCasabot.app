const int left = 32;   // change per sensor
const int center = 33;
const int right = 26;  // your “problem” sensor

void setup() { Serial.begin(115200); }
void loop() {
  Serial.print("Left: "); Serial.print(analogRead(left));
  Serial.print(" | Center: "); Serial.print(analogRead(center));
  Serial.print(" | Right: "); Serial.println(analogRead(right));
  delay(200);
}
